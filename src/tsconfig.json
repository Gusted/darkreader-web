{
    "compilerOptions": {
        "target": "es2019",
        "baseUrl": ".",
        "module": "es2015",
        "moduleResolution": "node",
        "lib": [
            "es2015",
            "es2017",
            "dom",
            "dom.iterable"
        ],
        "allowJs": true,
        "downlevelIteration": true,
        "noEmit": true,
        "incremental": true
    },
    "exclude": [
        "node_modules"
    ]
}
